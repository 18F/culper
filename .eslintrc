{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "import/no-duplicates": "off",
    "no-console": "off",
    "no-unused-vars": "off",
    "react/no-deprecated": "off",
    "react/no-string-refs": "off",
    "react/display-name": "off",
    "react/prop-types": [
      "error",
      {
        "skipUndeclared": true
      }
    ]
  },
  "settings": {
    "import/resolver": "webpack"
  }
}
