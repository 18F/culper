<DelinquencyInvolvingRoutineAccounts>
  <Answer>{{branch .props.HasNonpayment}}</Answer>
  <Comment></Comment>
  <Incidents>
    {{range $index, $item := .props.List.props.items}}
    {{with $Item := $item.Item}}
    <Incident ID="{{$index}}">
      <AccountNumber>{{text $Item.AccountNumber}}</AccountNumber>
      <Agency>
        <Name>{{text $Item.Name}}</Name>
      </Agency>
      <Amount Estimated="{{checkboxTrueFalse $Item.AmountEstimated}}">{{number $Item.Amount}}</Amount>
      <Comment></Comment>
      <DateBegan Type="{{dateEstimated $Item.Date}}">{{monthYear $Item.Date}}</DateBegan>
      <DateResolved Type="{{dateEstimated $Item.Resolved}}" NotResolved="{{notApplicable $Item.ResolvedNotApplicable}}">{{monthYear $Item.Resolved}}</DateResolved>
      <Description>{{textarea $Item.Description}}</Description>
      <IssueType>
        <Answer>{{if checkbox $Item.Infractions}}Yes{{else}}No{{end}}</Answer>
        <Options Property="{{checkboxHas $Item.Infractions "Repossession"}}" Loan="{{checkboxHas $Item.Infractions "Defaulted"}}" Bills="{{checkboxHas $Item.Infractions "Collections"}}" CCcard="{{checkboxHas $Item.Infractions "Cancelled"}}" Evicted="{{checkboxHas $Item.Infractions "Evicted"}}" Wages="{{checkboxHas $Item.Infractions "Garnished"}}" PastDeliquency="{{checkboxHas $Item.Infractions "Delinquent"}}" CurrentDeliquency="{{checkboxHas $Item.Infractions "Any"}}" />
      </IssueType>
      <PropertyType>{{text $Item.PropertyType}}</PropertyType>
      <Reason>{{textarea $Item.Reason}}</Reason>
      <Status>{{text $Item.Status}}</Status>
    </Incident>
    {{end}}
    {{end}}
  </Incidents>
  {{- if branch .props.HasNonpayment | eq "Yes"}}
  <HaveAdditionalEntryAnswer>{{branch .props.List.props.branch}}</HaveAdditionalEntryAnswer>
  <SummaryComment></SummaryComment>
  {{end}}
</DelinquencyInvolvingRoutineAccounts>
