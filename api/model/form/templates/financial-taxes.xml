<UnpaidTaxes>
  <Answer>{{branch .HasTaxes}}</Answer>
  <Comment></Comment>
  <HaveAdditionalEntryAnswer>{{branch .List.Branch}}</HaveAdditionalEntryAnswer>
  <Incidents>
    {{range $index, $item := .List.Items}}
    <Incident ID="{{$index}}">
      <!--Optional:-->
      <Actions>string</Actions>
      <Agency>{{text $item.Agency}}</Agency>
      <Amount Estimated="{{checkboxTrueFalse $item.AmountEstimated}}">{{number $item.Amount}}</Amount>
      <Comment></Comment>
      <DateFailedToPay Type="{{dateEstimated $item.YearEstimated}}">
        <Year>{{number $item.Year}}</Year>
      </DateFailedToPay>
      <DateSatisfied Type="{{dateEstimated $item.Date}}" NotApplicable="{{notApplicable $item.DateNotApplicable}}">{{monthYear $item.Date}}</DateSatisfied>
      <Reason>{{textarea $item.Reason}}</Reason>
      <TaxType>{{text $item.TaxType}}</TaxType>
      <Type>{{radio $item.Failure}}</Type>
    </Incident>
    {{end}}
  </Incidents>
  <SummaryComment></SummaryComment>
</UnpaidTaxes>
