<InvestigationRecord Version="1" Type="Pooled" LastSummaryID="201" LastDynamicListID="201">
  <ClearanceDenials>
    {{range $index, $item := .props.List.props.items}}
    {{with $Item := $item.Item}}
    <ClearanceDenial ID="{{$index}}">
      <Agency>{{$Item.Agency}}</Agency>
      <Circumstances>{{$Item.Explanation}}</Circumstances>
      <ClearanceDate>
        <Date Type="{{dateEstimated $Item.Date}}">
          {{monthYear $Item.Date}}
        </Date>
      </ClearanceDate>
      <EntryComment></EntryComment>
    </ClearanceDenial>
    {{end}}
    {{end}}
    <Comment></Comment>
    <SummaryComment></SummaryComment>
  </ClearanceDenials>

  <ClearanceDenied>
    <Answer>{{branch .props.HasRevocations}}</Answer>
    <HaveAdditionalEntryAnswer>{{branch .props.List.props.branch}}</HaveAdditionalEntryAnswer>
  </ClearanceDenied>

  <GovernmentDebarment>
    <Answer>{{branch .props.HasDebarment}}</Answer>
    <HaveAdditionalEntryAnswer>{{branch .props.List.props.branch}}</HaveAdditionalEntryAnswer>
  </GovernmentDebarment>

  <GovernmentDebarments>
    <Comment></Comment>
    {{range $index, $item := .props.List.props.items}}
    {{with $Item := $item.Item}}
    <GovernmentDebarment ID="{{$index}}">
      <Agency>{{text $Item.Agency}}</Agency>
      <Comment></Comment>
      <Date Type="{{dateEstimated $Item.Date}}">
        {{monthYear $Item.Date}}
      </Date>
      <Explanation>{{text $Item.Explanation}}</Explanation>
    </GovernmentDebarment>
    {{end}}
    {{end}}
    <SummaryComment></SummaryComment>
  </GovernmentDebarments>

  <Investigations>
    <Comment></Comment>
    {{range $index, $item := .props.List.props.items}}
    {{with $Item := $item.Item}}
    <Investigation ID="{{$index}}">
      <Agency>{{text $Item.Agency}}</Agency>
      <ClearanceLevel>{{text $Item.Clearance}}</ClearanceLevel>
      <EntryComment></EntryComment>
      <GrantedDate>
        <Date Type="{{dateEstimated $Item.Granted}}" DoNotKnow="{{notApplicable $Item.Granted}}">
          {{monthYear $Item.Granted}}
        </Date>
      </GrantedDate>

      <InvestigationDate>
        <Date Type="{{dateEstimated $Item.Completed}}" DoNotKnow="{{notApplicable $Item.Completed}}">
          {{monthYear $Item.Completed}}
        </Date>
      </InvestigationDate>
      <IssuingAgency>
        <Name>{{text $Item.Issued}}</Name>
      </IssuingAgency>
      <OtherAgency></OtherAgency>
      <OtherClearanceLevel></OtherClearanceLevel>
    </Investigation>
    {{end}}
    {{end}}
    <SummaryComment></SummaryComment>
  </Investigations>

  <PriorInvestigation>
    <Answer>{{branch .props.HasHistory}}</Answer>
    <HaveAdditionalEntryAnswer>{{branch .props.List.props.branch}}</HaveAdditionalEntryAnswer>
  </PriorInvestigation>
</InvestigationRecord>
