<AssociationRecord>
  <ActivitiesToOverthrowTheUSGovernment>
    <Activities>
      {{range $index, $item := .ActivitiesToOverthrow.props.List.props.items}}
      {{with $Item := $item.Item}}
      <Activity ID="{{$index}}">
        <Comment></Comment>
        {{daterange $Item.Dates}}
        <Reason>{{text $Item.Reasons}}</Reason>
      </Activity>
      {{end}}
      {{end}}
    </Activities>
    <Answer>{{branch .ActivitiesToOverthrow.props.HasActivities}}</Answer>
    <Comment></Comment>
    <HaveAdditionalEntryAnswer>{{branch .ActivitiesToOverthrow.props.List.props.branch}}</HaveAdditionalEntryAnswer>
    <SummaryComment></SummaryComment>
  </ActivitiesToOverthrowTheUSGovernment>

  <AdvocatedTerrorism>
    <Answer>{{branch .props.HasAdvocated}}</Answer>
    <Comment></Comment>
    <HaveAdditionalEntryAnswer>{{branch .Advocating.props.List.props.branch}}</HaveAdditionalEntryAnswer>

    <Instances>
      {{range $index, $item := .Advocating.props.List.props.items}}
      {{with $Item := $item.Item}}
      <Instance ID="{{$index}}">
        <Comment></Comment>
        {{daterange $Item.Dates}}
        <Reason>{{text $Item.Reason}}</Reason>
      </Instance>
      {{end}}
      {{end}}
    </Instances>
    <SummaryComment></SummaryComment>
  </AdvocatedTerrorism>

  <!--Optional:-->
  <BeenInOrganizationAdvocatingViolence>
    <Answer>{{branch .props.HasViolence}}</Answer>
    <Comment></Comment>
    <HaveAdditionalEntryAnswer>{{branch .MembershipViolence.props.List.props.branch}}</HaveAdditionalEntryAnswer>
    <Organizations>
      {{range $index, $item := .MembershipViolence.props.List.props.items}}
      {{with $Item := $item.Item}}
      <Organization ID="{{$index}}">
        <Address>
          {{location $Item.Address}}
        </Address>
        <Comment></Comment>
        <Contributions NoContributionsMade="{{notApplicable $Item.ContributionsNotApplicable}}">
          {{text $Item.Contributions}}
        </Contributions>

        {{daterange $Item.Dates}}
        <Description>{{text $Item.Reasons}}</Description>
        <Name>{{text $Item.Organization}}</Name>
        <PositionsHeld NoPositionsHeld="{{notApplicable $Item.PositionsNotApplicable}}">
          {{text $Item.Positions}}
        </PositionsHeld>
      </Organization>
      {{end}}
      {{end}}
    </Organizations>
    <SummaryComment></SummaryComment>
  </BeenInOrganizationAdvocatingViolence>

  <BeenInOrganizationDedicatedToTerrorism>
    <Answer>{{branch .TerroristOrganization.props.HasOtherOffenses}}</Answer>
    <Comment></Comment>
    <HaveAdditionalEntryAnswer>{{branch .TerroristOrganization.props.List.props.branch}}</HaveAdditionalEntryAnswer>
    <Organizations>
      {{range $index, $item := .props.List.props.items}}
      {{with $Item := $item.Item}}
      <Organization ID="{{$index}}">
        <Address>
          {{location $Item.Address}}
        </Address>
        <Comment></Comment>
        <Contributions NoContributionsMade="{{notApplicable $Item.ContributionsNotApplicable}}">
          {{text $Item.Contributions}}
        </Contributions>

        {{daterange $Item.Dates}}
        <Description>{{text $Item.Reasons}}</Description>
        <Name>{{text $Item.Organization}}</Name>
        <PositionsHeld NoPositionsHeld="{{notApplicable $Item.PositionsNotApplicable}}">
          {{text $Item.Positions}}
        </PositionsHeld>
      </Organization>
      {{end}}
      {{end}}
    </Organizations>
    <SummaryComment></SummaryComment>
  </BeenInOrganizationDedicatedToTerrorism>

  <!--Optional:-->
  <EngagedInTerrorism>
    <Answer>{{branch .EngagedInTerrorism.props.HasEngaged}}</Answer>
    <Comment></Comment>
    <HaveAdditionalEntryAnswer>{{branch .EngagedInTerrorism.props.List.props.branch}}</HaveAdditionalEntryAnswer>
    <SummaryComment></SummaryComment>
    <TerroristActs>
      {{range $index, $item := .EngagedInTerrorism.props.List.props.items}}
      {{with $Item := $item.Item}}
      <Act ID="{{$index}}">
        <Comment></Comment>
        {{daterange $Item.Dates}}
        <Reason>{{text $Item.Reasons}}</Reason>
      </Act>
      {{end}}
      {{end}}
    </TerroristActs>
  </EngagedInTerrorism>

  <TerrorismAssociations>
    <Answer>{{branch .TerrorismAssociation.props.HasTerrorism}}</Answer>
    <AssociationComment></AssociationComment>
    <Comment></Comment>
    <Explanation>{{text .TerrorismAssociation.props.Explanation}}</Explanation>
  </TerrorismAssociations>

  <TerroristOrganizationMember>
    <Answer>{{branch .MembershipOverthrow.props.HasAdvocated}}</Answer>
    <Comment></Comment>
    <HaveAdditionalEntryAnswer>{{branch .MembershipOverthrow.props.List.props.branch}}</HaveAdditionalEntryAnswer>
    <Organizations>
      {{range $index, $item := .props.List.props.items}}
      {{with $Item := $item.Item}}
      <Organization ID="{{$index}}">
        <Address>
          {{location $Item.Address}}
        </Address>
        <Comment></Comment>
        <Contributions NoContributionsMade="{{notApplicable $Item.ContributionsNotApplicable}}">{{text $Item.Contributions}}</Contributions>
        {{daterange $Item.Dates}}
        <Description>{{text $Item.Reason}}</Description>
        <Name>{{text $Item.Organization}}</Name>
        <PositionsHeld NoPositionHeld="{{notApplicable $Item.PositionsNotApplicable}}">{{text $Item.Position}}</PositionsHeld>
      </Organization>
      {{end}}
      {{end}}
    </Organizations>
    <SummaryComment></SummaryComment>
  </TerroristOrganizationMember>
</AssociationRecord>
