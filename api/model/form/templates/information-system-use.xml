<InformationSystemUse Version="1" Type="Pooled" LastSummaryID="201" LastDynamicListID="201">
  <IllegalEntry>
    <Answer>{{branch .props.HasUnauthorized}}</Answer>
    <HaveAdditionalEntryAnswer>{{branch .props.List.props.branch}}</HaveAdditionalEntryAnswer>
    <Comment></Comment>
    <Incidents>
      {{range $index, $item := .props.List.props.items}}
      {{with $Item := $item.Item}}
      <Incident ID="{{$index}}">
        <Action>{[text $Item.Action}}</Action>
        <Address>
          {{location $Item.Location}}
        </Address>
        <Comment></Comment>
        <Date Type="{{dateEstimated $Item.Date}}">
          {{monthYear $Item.Date}}
        </Date>
        <Nature>{{text $Item.Incident}}</Nature>
      </Incident>
      {{end}}
      {{end}}
    </Incidents>
    <SummaryComment></SummaryComment>
  </IllegalEntry>

  <IllegalUse>
    <Answer>{{branch .props.HasUnlawful}}</Answer>
    <HaveAdditionalEntryAnswer>{{branch .props.List.props.branch}}</HaveAdditionalEntryAnswer>
    <Incidents>
      <Incident ID="{{$index}}">
        <Action>{[text $Item.Action}}</Action>
        <Address>
          {{location $Item.Location}}
        </Address>
        <Comment></Comment>
        <Date Type="{{dateEstimated $Item.Date}}">
          {{monthYear $Item.Date}}
        </Date>
        <Nature>{{text $Item.Incident}}</Nature>
      </Incident>
    </Incidents>
    <SummaryComment></SummaryComment>
  </IllegalUse>

  <IllegallyAlteredInformation>
    <Answer>{{branch .props.HasManipulating}}</Answer>
    <HaveAdditionalEntryAnswer>{{branch .props.List.props.branch}}</HaveAdditionalEntryAnswer>
    <Comment></Comment>
    <Incidents>
      <Incident ID="{{$index}}">
        <Action>{[text $Item.Action}}</Action>
        <Address>
          {{location $Item.Location}}
        </Address>
        <Comment></Comment>
        <Date Type="{{dateEstimated $Item.Date}}">
          {{monthYear $Item.Date}}
        </Date>
        <Nature>{{text $Item.Incident}}</Nature>
      </Incident>
    </Incidents>
    <SummaryComment></SummaryComment>
  </IllegallyAlteredInformation>
</InformationSystemUse>
