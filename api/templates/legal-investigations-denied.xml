{{- if branch .props.HasRevocations | eq "Yes"}}
<ClearanceDenials>
  {{range $index, $item := .props.List.props.items}}
  {{with $Item := $item.Item}}
  <ClearanceDenial ID="{{inc $index}}">
    <Agency>{{text $Item.Agency}}</Agency>
    <Circumstances>{{text $Item.Explanation}}</Circumstances>
    <ClearanceDate>
      <Date Type="{{dateEstimated $Item.Date}}">
        {{monthYear $Item.Date}}
      </Date>
    </ClearanceDate>
    <EntryComment></EntryComment>
  </ClearanceDenial>
  {{end}}
  {{end}}
  <Comment></Comment>
  <SummaryComment></SummaryComment>
</ClearanceDenials>
{{end}}

<ClearanceDenied>
  <Answer>{{branch .props.HasRevocations}}</Answer>
  {{- if branch .props.HasRevocations | eq "Yes"}}
  <HaveAdditionalEntryAnswer>{{branch .props.List.props.branch}}</HaveAdditionalEntryAnswer>
  {{end}}
</ClearanceDenied>
