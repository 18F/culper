<TerroristOrganizationMember>
  <Answer>{{branch .props.HasOverthrow}}</Answer>
  <Comment></Comment>
  <Organizations>
    {{range $index, $item := .props.List.props.items}}
    {{with $Item := $item.Item}}
    <Organization ID="{{$index}}">
      <Address>
        {{location $Item.Address}}
      </Address>
      <Comment></Comment>
      <Contributions NoContributionsMade="{{notApplicable $Item.ContributionsNotApplicable}}">{{text $Item.Contributions}}</Contributions>
      {{monthYearDaterange $Item.Dates}}
      <Description>{{text $Item.Reasons}}</Description>
      <Name>{{text $Item.Organization}}</Name>
      {{if notApplicable $Item.PositionsNotApplicable | eq "True"}}
      <PositionsHeld NoPositionsHeld="True" />
      {{else}}
      <PositionsHeld>
        {{text $Item.Positions}}
      </PositionsHeld>
      {{end}}
    </Organization>
    {{end}}
    {{end}}
  </Organizations>
  {{- if branch .props.HasOverthrow | eq "Yes"}}
  <HaveAdditionalEntryAnswer>{{branch .props.List.props.branch}}</HaveAdditionalEntryAnswer>
  <SummaryComment></SummaryComment>
  {{end}}
</TerroristOrganizationMember>
